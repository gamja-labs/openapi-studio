{
  "name": "openapi-studio",
  "version": "0.6.0",
  "private": true,
  "scripts": {
    "build": "yarn workspaces foreach --topological --all --exclude nestjs-test-project run build",
    "ci": "yarn build",
    "dev": "yarn workspace @openapi-studio/core run dev"
  },
  "workspaces": [
    "core",
    "desktop",
    "nestjs",
    "nestjs-test-project"
  ],
  "devDependencies": {
    "typescript": "~5.9.3"
  },
  "packageManager": "yarn@4.12.0"
}
